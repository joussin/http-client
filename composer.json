{
    "name": "joussin/http-client",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/joussin/http-client",
    "description": "A psr http-client implementation",
    "keywords": ["package"],
    "authors": [
        {
            "name": "Joussin StÃ©phane",
            "email": "joussin@live.com",
            "role": "Developer"
        }
    ],

    "require": {
        "php": ">=7.2.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0"
    },

    "require-dev": {
        "symfony/var-dumper": "^6.3"
    },

    "autoload": {
        "psr-4": {
            "Joussin\\Component\\HttpClient\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "laravel": {}
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "conflict": {},
    "suggest": {},
    "scripts": {},
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
